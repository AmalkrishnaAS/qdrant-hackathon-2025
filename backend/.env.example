# Application Settings
SECRET_KEY=your-secret-key-here
DEBUG=True

# Qdrant Configuration
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_API_KEY=your-qdrant-api-key
QDRANT_COLLECTION_NAME=music_embeddings

# Upstash Configuration (Required for Redis)
UPSTASH_REDIS_REST_URL=your-upstash-redis-rest-url
UPSTASH_REDIS_REST_TOKEN=your-upstash-redis-rest-token

# Celery Configuration
CELERY_BROKER_URL="rediss://default:your-upstash-redis-password@your-upstash-instance.upstash.io:6379?ssl_cert_reqs=CERT_NONE"
CELERY_RESULT_BACKEND="rediss://default:your-upstash-redis-password@your-upstash-instance.upstash.io:6379?ssl_cert_reqs=CERT_NONE"

# File Upload Configuration
UPLOAD_FOLDER=uploads
MAX_CONTENT_LENGTH=104857600  # 100MB in bytes

# CORS Configuration (comma-separated origins)
CORS_ORIGINS=http://localhost:3000,http://localhost:3001

# Embedding Model Configuration
SENTENCE_TRANSFORMER_MODEL=all-MiniLM-L6-v2
